        
        <div class="search-map">
                    
                 <div id="map">

                 </div>

         </div>
        
        <script type="text/javascript">
          function initialize(){
                bulidMap();
          }
          
          function bulidMap(){
                var hashes = '<%= raw hash.to_json %>';
                handler = Gmaps.build('Google');
                handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
                  initMapMarker(hashes);
                });
                setMapStyle(
                  getLocationData(hashes)
                );
          }
        </script>
       